{
    "name": "Search",
    "description": "Search Chrome extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "options_page": "/options/options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["/react/main.css"],
            "js": ["/assets/js/initializeUI.js"],
            "run_at": "document_end",
            "all_frame": true
        }
    ],
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "background": {
        "service_worker": "/assets/js/service-worker.js"
    },
    "externally_connectable": {
        "matches": ["<all_urls>"]
    },
    "web_accessible_resources": [
        {
            "resources": ["/react/main.js", "/react/main.css"],
            "matches": ["<all_urls>"]
        }
    ]
}